{
    "name": "maymeow/base-int-encoder",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Maymeow\\BaseIntEncoder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maymeow\\BaseIntEncoder\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "MayMeow",
            "email": "emma@themaymeow.com"
        }
    ],
    "require": {
        "php": "8.0.*",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^0.12.99"
    },
    "scripts": {
        "test": "phpunit tests",
        "cs-check": "phpcs --standard=PSR2 src",
        "cs-fix": "phpcbf --standard=PSR2 src",
        "stan": "phpstan analyse"
    }
}
